# ===========================
# Database Configuration
# ===========================
# Full connection string for PostgreSQL
DATABASE_URL=postgres://your_username:your_password@your_host:23150/your_db?sslmode=require

# Individual DB connection parts (optional, use if needed separately)
DB_USER=your_username
DB_PASSWORD=your_password
DB_HOST=your_host
DB_PORT=23150
DB_NAME=your_db

# ===========================
# Swagger Docs
# ===========================
# Swagger server base URL
SWAGGER_SERVER_URL=http://localhost:5001

# ===========================
# Hyperbase Config
# ===========================
HYPERBASE_PROJECT_ID=your_project_id
HYPERBASE_TOKEN_ID=your_token_id
HYPERBASE_COLLECTION_ID=your_collection_id

# ===========================
# MQTT Broker Config
# ===========================
MQTT_BROKER_URL=ws://your_broker_ip:port/mqtt
MQTT_TOPIC=your_topic
